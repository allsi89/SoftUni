{{> header}}
<div class="col-md-6 text-center col-lg">
    <img class="" src="{{profilePic}}"/>
    <div class="profile-info">
        <p>Username:
            <small>{{username}}</small>
        </p>
        <p class="infoType">Creator of {{books.length}} books.</p>
        {{#each books}}
            <p>{{name}}</p>
        {{else}}
            <p>No books</p>
        {{/each}}
    </div>
</div>
{{> footer}}